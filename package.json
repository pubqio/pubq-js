{
    "name": "pubq",
    "version": "1.0.2",
    "description": "PUBQ JavaScript SDK",
    "main": "build/node/pubq.js",
    "browser": "build/web/pubq.js",
    "scripts": {
        "build": "rm -rf build/ && npm run build:node && npm run build:web",
        "build:node": "rm -rf types/ && tsc && rm -rf build/node/types/",
        "build:web": "npx webpack && npx webpack --mode production"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/pubqio/pubq-js.git"
    },
    "keywords": [
        "pubq",
        "pubsub",
        "channels",
        "websocket",
        "realtime",
        "messaging",
        "communication"
    ],
    "author": "PUBQ Team",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/pubqio/pubq-js/issues"
    },
    "homepage": "https://pubq.io",
    "dependencies": {
        "axios": "^1.5.0",
        "eventemitter3": "^5.0.1",
        "socketcluster-client": "^17.1.1"
    },
    "devDependencies": {
        "ts-loader": "^9.4.4",
        "typescript": "^5.2.2",
        "webpack": "^5.80.0",
        "webpack-cli": "^5.0.2"
    },
    "engines": {
        "node": ">=5.10.x"
    }
}
